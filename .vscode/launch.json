{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch via NPM",
			"request": "launch",
			"runtimeArgs": [
				"example",
				"${relativeFile}"
				// "./examples/src/llms/hf.ts"
			],
			"runtimeExecutable": "yarn",
			"outFiles": [
				"${workspaceFolder}/**/*.js",
				"!**/node_modules/**"
			],
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "node",
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Program",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/examples/src/llms/hf.ts",
			"outFiles": [
				"${workspaceFolder}/**/*.js"
			]
		}
	]
}